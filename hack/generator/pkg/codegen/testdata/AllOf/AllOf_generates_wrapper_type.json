{
    "$comment": "Check that the AllOf handler correctly generates a union type. Note that this will also generate the standalone types Foo, Bar, and Baz which might actually be a bug (see discussion: https://github.com/Azure/k8s-infra/issues/114)" ,
    "id": "https://test.test/schemas/2020-01-01/test.json",
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "Test",
    "type": "object",
    "allOf": [
        {
            "$ref": "#/definitions/Foo"
        },
        {
            "$ref": "#/definitions/Bar"
        },
        {
            "$ref": "#/definitions/Baz"
        }
    ],
    "definitions": {
        "Foo": {
            "type": "object",
            "properties": {
                "name": {
                    "type": "string"
                }
            }
        },
        "Bar": {
            "type": "object",
            "properties": {
                "size": {
                    "type": "integer"
                }
            },
            "required": [
                "size"
            ]
        },
        "Baz": {
            "type": "object",
            "properties": {
                "enabled": {
                    "type": "boolean"
                }
            },
            "required": [
                "enabled"
            ]
        }
    }
}